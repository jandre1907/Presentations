{# Confirmation porteur différent payeur #}
<div class="step_form" id="confirmation" data-ng-show="blockNumber == '1111'" data-ng-form name="step_form">
    <div class="confirmation">
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12">
            {{"sna.sna_confirmation_porteur.bloc_debut"|trans({}, "sna", "fr")|raw}}
            </div>
        </div>
        <div class="form-group">
            <div class="col-xs-11 col-sm-12 col-md-12 text-center">
                <button type="button" class="btn btn-primary aide-photo-btn" onclick="location.href='./login'">
                    <span>{{"sna.sna_confirmation_porteur.bouton_creer_compte"|trans({}, "sna", "fr")|raw}}</span>
                </button>
            </div>
        </div>
    </div>
</div>

