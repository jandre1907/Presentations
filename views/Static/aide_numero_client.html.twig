{% extends "SelBundle::layout.html.twig" %}

{% block contentTitle %}
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12">
            <div class="heading">
            {% if app.request.attributes.get('courseType') == 'commande_de_carte' %}
                <h1>{{'static.static_aide_numero_client.sna_title'|trans({}, 'static', 'fr') }}</h1>
            {% else %}
                <h1>{{'static.static_aide_numero_client.sna_title'|trans({}, 'static', 'fr') }}</h1>
            {% endif %}
            </div>
        </div>
    </div>
{% endblock contentTitle %}

{% block breadcrumb %}
    <div>
        <div id="box_breadcrumb_id" class="row">
            <div class="col-xs-12 col-sm-12 col-md-12">
                <ol class="breadcrumb">
                    <li><a href="http://www.navigo.fr" title="{{ 'static.static_aide_numero_client.accueil_link_breadcrumb_title'|trans({}, 'static') }}">{{ 'static.static_aide_numero_client.accueil_link_breadcrumb'|trans({}, 'static') }}</a></li>
                    <li><a href="{{ path('sel_homepage') }}" title="{{ 'static.static_aide_numero_client.gere_maCarte_link_breadcrumb_title'|trans({}, 'static') }}">{{ 'static.static_aide_numero_client.gere_maCarte_link_breadcrumb'|trans({}, 'static') }}</a></li>
                    <li class="active">{{ 'static.static_aide_numero_client.souscription_forfait_link_breadcrumb'|trans({}, 'static') }}</li>
                </ol>
            </div>
        </div>
    </div>
{% endblock breadcrumb %}


