{% verbatim %}
<nav id="box_nav_step"  data-ng-controller="BreadcrumbCtrl as bread" data-ng-model="hide" data-ng-hide="hide">
  <ul>
      <li>
          <!-- Start heading -->

          <div class="box_titre" data-ng-controller="TitleCtrl as titleCtrl">
              <a id="box_nav_step_action" href="#box_nav_step">
                  <h2>{{titleCtrl.title}}</h2>
                  <span class="box_open_close">
                      {% endverbatim %}<img src="{{ asset('bundles/sel/images/nav_arrow_top.png') }}" alt="Ouvrir le menu des Ã©tapes"/>{% verbatim %}
                  </span>
              </a>
          </div>
