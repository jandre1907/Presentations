          <!-- Start progress-bar -->
          <div id="box_nav_step_progressbar">
              <div id="box_nav_step_wherein_progressbar"></div>
          </div>
          <!-- End progress-bar -->
          <!-- Start step-bar -->
          <div id="box_nav_step_sub_menu">
            <ol>
              <li data-ng-class="{
                'selected':isActive('/Forfait'),
                'complete':isComplete('/Forfait'),
                'disabled':isDisabled('/Forfait')
              }">
                <a class="link selected_link" href="#Forfait" title="{% endverbatim %}{{"sna.sna_transverse.step_forfait_title"|trans({}, "sna", "fr")|raw}}{% verbatim %}" data-ng-show="hasRightToGoTo('/Forfait')">
                    <span class="box_img">
                        {% endverbatim %}<img src="{{ asset('bundles/sel/images/nav_picto_ok.png') }}" alt="Etape fini"/>{% verbatim %}
                    </span>
                    {% endverbatim %}{{"sna.sna_transverse.step_forfait"|trans({}, "sna", "fr")|raw}}{% verbatim %}
                </a>
                <span class="box_no_link" data-ng-hide="hasRightToGoTo('/Forfait')">{% endverbatim %}{{"sna.sna_transverse.step_forfait"|trans({}, "sna", "fr")|raw}}{% verbatim %}</span>

