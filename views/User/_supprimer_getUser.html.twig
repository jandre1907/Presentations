{% extends "SelBundle::layout.html.twig" %}

{% block title "Symfony - User" %}

{% block content_header '' %}

{% block content %}
    <h1 class="title">User #{{ id }}</h1>

    <p>{{ user.username }}</p>
    <p>{{ user.email }}</p>
    <p>
        <a href="{{ path('get_users') }}">{{ 'user.list.link'|trans({}, 'SelBundle') }}</a> <br>
    </p>
{% endblock %}


