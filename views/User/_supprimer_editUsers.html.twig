{% extends "SelBundle::layout.html.twig" %}

{% block title "Symfony - Users" %}

{% block content_header '' %}

{% block content %}
    <h1 class="title">{{ 'user.edit.headline'|trans({'%id%': id}, 'SelBundle') }}</h1>

    <form action="{{ url('put_users', {'id': id}) }}" method="POST" {{ form_enctype(form) }}>
        <input type="hidden" name="_method" value="put" />

        {{ form_widget(form) }}

        <input type="submit" value="{% trans from 'SelBundle' %}user.edit.submit{% endtrans %}">
    </form>
{% endblock %}


